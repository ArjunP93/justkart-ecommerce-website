    <script>

        function deleteAddress(id) {

            swal({
                title: "Are you sure?",
                text: "Once deleted, you will not be able to recover the address!",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {
                        $.ajax({
                            url: '/delete-address',
                            type: 'delete',
                            data: {
                                id: id
                            },
                            success: function (response) {
                                if (response) {
                                    location.reload()
                                }
                            }
                        })
                        swal("Your address is deleted!", {
                            icon: "success",
                        });
                    } else {
                        swal("Canceled");
                    }
                });
        }





    </script>